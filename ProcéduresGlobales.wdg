   -
     name : Liste_PersonnesActives
     procedure_id : 1353347261589243554
     type_code : 15
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] Liste_PersonnesActives ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	tableau (MPersonne) : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure Liste_PersonnesActives(): tableau de CL_Personne
      
      vtab_pers			est un tableau de CL_Personne
      vcl_pers			est une CL_Personne
      vcl_instrument		est un CL_Instrument
      vs_nomEnCours		est une chaine
      vn_ind				est un entier
      
      HExécuteRequête(REQ_ListePersonnesActives)
      POUR TOUTE REQ_ListePersonnesActives
      	si vs_nomEnCours <> REQ_ListePersonnesActives.Nom + " " +  REQ_ListePersonnesActives.Prenom ALORS
      		VariableRAZ(vcl_pers)
      		FichierVersMémoire(vcl_pers, REQ_ListePersonnesActives)
      		vcl_pers.m_sRecherche = Majuscule(vcl_pers.m_sPrenom) +";" + Majuscule(vcl_pers.m_sNom) +";" + Majuscule(vcl_pers.m_sEMail)
      
      		SI REQ_ListePersonnesActives.ID_Instrument <> 0 ALORS
      			VariableRAZ(vcl_instrument)
      			vcl_instrument.m_ID				= REQ_ListePersonnesActives.ID_Instrument
      			vcl_instrument.m_sInstrument	= REQ_ListePersonnesActives.Instrument
      			vcl_instrument.m_sImage			= GET_ImageInstrument(vcl_instrument.m_ID)
      			vcl_pers.m_sRecherche += [";"] + Majuscule(vcl_instrument.m_sInstrument)
      			vcl_pers.mtab_instruments.AjouteLigne(vcl_instrument)
      		FIN
      		vn_ind			= TableauAjouteLigne(vtab_pers,vcl_pers)
      		vs_nomEnCours	= REQ_ListePersonnesActives.Nom + " " +  REQ_ListePersonnesActives.Prenom
      	SINON
      		vtab_pers[vn_ind].Ajouter_Instrument(REQ_ListePersonnesActives.ID_Instrument)
      		vtab_pers[vn_ind].m_bMultipleInstruments = Vrai
      		vtab_pers[vn_ind].m_sRecherche += [";"] + Majuscule(REQ_ListePersonnesActives.Instrument)
      	FIN
      FIN
      
      RENVOYER vtab_pers
     type : 458752
