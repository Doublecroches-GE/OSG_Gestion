     name : Liste_Personnes
     procedure_id : 1749658582781262527
     type_code : 15
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] Liste_Personnes ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	tableau (CL_Personne) : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure Liste_Personnes(): tableau de CL_Personne
      
      vtab_pers			est un tableau de CL_Personne
      vcl_pers			est une CL_Personne
      vcl_instrument		est un CL_Instrument
      vs_nomEnCours		est une chaine
      vn_ind				est un entier
      
      HExécuteRequête(REQ_ListePersonnes)
      POUR TOUTE REQ_ListePersonnes
      	si vs_nomEnCours <> REQ_ListePersonnes.Nom + " " +  REQ_ListePersonnes.Prenom ALORS
      		VariableRAZ(vcl_pers)
      		FichierVersMémoire(vcl_pers, REQ_ListePersonnes)
      		vcl_pers.m_sRecherche = Majuscule(vcl_pers.m_sPrenom) +";" + Majuscule(vcl_pers.m_sNom) +";" + Majuscule(vcl_pers.m_sEMail)
      
      		SI REQ_ListePersonnes.ID_Instrument <> 0 ALORS
      			VariableRAZ(vcl_instrument)
      			vcl_instrument.m_ID				= REQ_ListePersonnes.ID_Instrument
      			vcl_instrument.m_sInstrument	= REQ_ListePersonnes.Instrument
      			vcl_instrument.m_sImage			= GET_ImageInstrument(vcl_instrument.m_ID)
      			vcl_pers.m_sRecherche += [";"] + Majuscule(vcl_instrument.m_sInstrument)
      			vcl_pers.mtab_instruments.AjouteLigne(vcl_instrument)
      		FIN
      		vn_ind			= TableauAjouteLigne(vtab_pers,vcl_pers)
      		vs_nomEnCours	= REQ_ListePersonnesActives.Nom + " " +  REQ_ListePersonnes.Prenom
      	SINON
      		vtab_pers[vn_ind].Ajouter_Instrument(REQ_ListePersonnes.ID_Instrument)
      		vtab_pers[vn_ind].m_bMultipleInstruments = Vrai
      		vtab_pers[vn_ind].m_sRecherche += [";"] + Majuscule(REQ_ListePersonnes.Instrument)
      	FIN
      FIN
      
      RENVOYER vtab_pers
     type : 458752
