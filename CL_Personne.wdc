     procedure_id : 1362620572731575812
     type_code : 12
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Ajouter_Instrument (<vn_id>)
      //
      // Paramètres :
      // 	vn_id : <indiquez ici le rôle de vn_id>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure Ajouter_Instrument(vn_id)
      
      vcl_instrument est une CL_Instrument
      
      SI HLitRecherchePremier(Instrument,ID,vn_id) ALORS 
      	FichierVersMémoire(vcl_instrument,Instrument)
      	mtab_instruments.AjouteLigne(vcl_instrument)
