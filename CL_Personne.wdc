   -
     name : Sauver
     procedure_id : 1367829101088454903
     type_code : 12
     code : |1-
      // Redéfinition de la méthode CL_Gestion.Sauver
      PROCÉDURE PUBLIQUE Sauver() 
      
      vcl_persInstr	est une CL_Personne_Instrument
      vb_creation		est un booléen
      
      vb_creation = m_ID = 0
      Ancêtre.Sauver()
      
      Si vb_creation ALORS
      	pour i = 1 A mtab_instruments..Occurrence
      		vcl_persInstr.m_IDInstrument	= mtab_instruments[i].m_ID
      		vcl_persInstr.m_IDPersonne		= m_Id
      		vcl_persInstr.Sauver_Personne_Instrument(vrai)
      	FIN
      FIN
